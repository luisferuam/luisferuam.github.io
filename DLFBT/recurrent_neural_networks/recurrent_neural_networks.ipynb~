      "source": [
        "r = RNN(3, 10)\n",
        "r2 = RNN_2(3, 10)\n",
        "\n",
        "r2.Wxh = tf.Variable(r.Wxh[None, :, :])\n",
        "r2.Whh = tf.Variable(r.Whh[None, :, :])\n",
        "r2.bh = tf.Variable(r.bh[None, :, :])\n",
        "r2.Why = tf.Variable(r.Why[None, :, :])\n",
        "r2.by = tf.Variable(r.by[None, :, :])\n",
        "\n",
        "y, _ = r.predict(x_one_hot[:, :20])\n",
        "y2, _ = r2.predict(x_one_hot[None, :, :20])\n",
