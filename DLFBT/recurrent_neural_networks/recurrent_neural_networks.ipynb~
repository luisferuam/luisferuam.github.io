    {
      "cell_type": "markdown",
      "metadata": {
        "id": "Pe3LzJO6089t"
      },
      "source": [
        "The same thing but operating in batches. When using ``tf.matmul`` the batch_size must be the tensor's first dimension (see https://www.tensorflow.org/api_docs/python/tf/linalg/matmul)."
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "5RGHh1A606aO"
      },
      "source": [
        "class RNN_2(object):\n",
        "\n",
        "  #-----------------------------------------------------------------------------\n",
        "  def __init__(self, ninput, nhidden):\n",
        "    self.ninput = ninput\n",
        "    self.nhidden = nhidden\n",
        "    self.noutput = 1\n",
        "\n",
        "    # Initial state:\n",
        "    self.reset_state()\n",
        "\n",
        "    # Weight matrices and bias vectors:\n",
        "    # I add one additional rank for the batch_size.\n",
        "    self.Wxh = tf.Variable(tf.random.normal(shape=[1, self.nhidden, self.ninput], dtype=tf.dtypes.float64))\n",
        "    self.Whh = tf.Variable(tf.random.normal(shape=[1, self.nhidden, self.nhidden], dtype=tf.dtypes.float64))\n",
        "    self.bh = tf.Variable(tf.random.normal(shape=[1, self.nhidden, 1], dtype=tf.dtypes.float64))\n",
        "    self.Why = tf.Variable(tf.random.normal(shape=[1, self.noutput, self.nhidden], dtype=tf.dtypes.float64))\n",
        "    self.by = tf.Variable(tf.random.normal(shape=[1, self.noutput, 1], dtype=tf.dtypes.float64))\n",
        "\n",
        "  #-----------------------------------------------------------------------------\n",
        "  def reset_state(self):\n",
        "    self.h = np.zeros((1, self.nhidden, 1))\n",
        "\n",
        "  #-----------------------------------------------------------------------------\n",
        "  def predict(self, x):\n",
        "    n = x.shape[2]\n",
        "    h = self.h\n",
        "    y = []\n",
        "    states = []\n",
        "    for i in range(n):\n",
        "      z = tf.matmul(self.Wxh, x[:, :, i:(i+1)]) + tf.matmul(self.Whh, h) + self.bh\n",
        "      h = tf.sigmoid(z)\n",
        "      z = tf.matmul(self.Why, h) + self.by\n",
        "      y.append(tf.sigmoid(z))\n",
        "      states.append(h)\n",
        "\n",
        "    self.h = h\n",
        "    return tf.concat(y, axis=2), states\n",
        "\n",
        "  #-----------------------------------------------------------------------------\n",
        "  def compute_gradients(self, x, t):\n",
        "    with tf.GradientTape(persistent=True) as tape: \n",
        "      y, _ = self.predict(x)\n",
        "      loss = -tf.reduce_mean(t*tf.math.log(y) + (1.-t)*tf.math.log(1.-y))\n",
        "      \n",
        "    dWxh = tape.gradient(loss, self.Wxh)\n",
        "    dWhh = tape.gradient(loss, self.Whh)\n",
        "    dbh = tape.gradient(loss, self.bh)\n",
        "    dWhy = tape.gradient(loss, self.Why)\n",
        "    dby = tape.gradient(loss, self.by)\n",
        "\n",
        "    return dWxh, dWhh, dbh, dWhy, dby\n",
        "\n",
        "  #-----------------------------------------------------------------------------\n",
        "  def gradient_step(self, x, t, eta):\n",
        "    dWxh, dWhh, dbh, dWhy, dby = self.compute_gradients(x, t)\n",
        "\n",
        "    self.Wxh.assign(self.Wxh - eta*dWxh)\n",
        "    self.Whh.assign(self.Whh - eta*dWhh)\n",
        "    self.bh.assign(self.bh - eta*dbh)\n",
        "    self.Why.assign(self.Why - eta*dWhy)\n",
        "    self.by.assign(self.by - eta*dby)\n",
        "\n",
        "  #-----------------------------------------------------------------------------\n",
        "  def fit(self, x, t, eta, num_epochs, seq_len):\n",
        "    \"\"\"\n",
        "    x must be (batch_size x dim x n)\n",
        "    t must be (batch_size x 1 x n)\n",
        "    \"\"\"\n",
        "    (batch_size, dim, n) = x.shape\n",
        "    num_sequences = (n // seq_len) # If less than seq_len examples remain they are ignored\n",
        "\n",
        "    loss = np.zeros(num_epochs)\n",
        "    for i in range(num_epochs):\n",
        "      # Reset state before each epoch:\n",
        "      self.reset_state()\n",
        "      for j in range(num_sequences):\n",
        "        imin = j*seq_len\n",
        "        imax = (j+1)*seq_len\n",
        "        \n",
        "        seq_x = x[:, :, imin:imax]\n",
        "        seq_t = t[:, :, imin:imax]\n",
        "        self.gradient_step(seq_x, seq_t, eta)\n",
        "\n",
        "      # Compute epoch loss with all data:\n",
        "      self.reset_state()\n",
        "      y, _ = self.predict(x)\n",
        "      loss[i] = -tf.reduce_mean(t*tf.math.log(y) + (1.-t)*tf.math.log(1.-y))\n",
        "      print(\"Epoch %d, loss = %f\" % (i, loss[i]))\n",
        "      \n",
        "    return loss\n",
        "\n"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "3T706ps506Nc",
        "outputId": "a59155e1-4217-4e22-dfb7-6d6a166a4e24",
