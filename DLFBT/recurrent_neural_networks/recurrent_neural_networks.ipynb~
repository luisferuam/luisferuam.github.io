      },
      "source": [
        "n = 50000\n",
        "seq_len = 25\n",
        "pattern = '11011'\n",
        "\n",
        "x, x_as_string, t = create_data_set(n, seq_len, pattern)\n",
        "xval, xval_as_string, tval = create_data_set(n, seq_len, pattern)\n",
        "\n",
        "for s, c in zip(x_as_string[:20], t[:20]):\n",
        "  print(s, c)\n",
        "\n",
        "print(t.mean())\n",
        "print(tval.mean())"
